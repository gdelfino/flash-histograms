package {    import flash.display.Sprite;    import skins.IrreversibleSkin;    import info.esdata.Histogram;    import info.esdata.DoubleSwitch;    import info.esdata.DataLoader;    import info.esdata.EtiquetaDinamica;    import info.esdata.Estado;    import info.esdata.Municipio;    import info.esdata.Parroquia;    import info.esdata.Centro;    import info.esdata.FullScreenButton;        public class Irreversible extends Sprite {        /*public static const COLOR_ESCONDIDAS : uint = 0xE7E354; */        /*public static const COLOR_ESCONDIDAS : uint = 0xFFFF66; //banana */        public static const COLOR_ESCONDIDAS : uint = 0xFFFFFF; //banana         /*public static const COLOR_PUBLICADAS : uint = 0x9E3EFF;*/        public static const COLOR_PUBLICADAS : uint = 0x8000FF;//grape        public var _h1 : Histogram = new Histogram(0, 100, 1  , COLOR_ESCONDIDAS, "con rayas"); // escondidas        public var _h2 : Histogram = new Histogram(0, 100, 0.5, COLOR_PUBLICADAS, "sin rayas"); // publicadas        public var edo : EtiquetaDinamica;        public var mun : EtiquetaDinamica;        public var par : EtiquetaDinamica;        public var ctr : EtiquetaDinamica;        public var msa : EtiquetaDinamica;        public var prc : EtiquetaDinamica;        private var _ds : DoubleSwitch; // Para cambiar entre encondidas y publicadas                public function Irreversible() {            //addChild(new IrreversibleSkin.ProjectSprouts());            addChild(_h1);            addChild(_h2);            _h1.mouseChildren = true;            _h2.mouseChildren = false;            _h1.verticalAxisLocation = 'right';                        _h1.xlabel("Porcentaje Oficialista en elección Presidencial de 2006");            _h2.xlabel("Porcentaje Oficialista en elección Presidencial de 2006");                        _h1.ylabel("Número de mesas aun sin publicar en 2007");            _h2.ylabel("Número de mesas publicadas en 2007");                        _h1.ymax = 118;            _h2.ymax = 401;                        _h1.drawAxes();            _h2.drawAxes();                        _ds = new DoubleSwitch(_h1, "Mesas con resultado 2007 no publicado por el CNE",                                   _h2, "Mesas con resultado 2007 publicado por el CNE");            addChild(_ds);            _ds.select("Mesas con resultado 2007 no publicado por el CNE");                        var dl : DataLoader = new DataLoader(_h1, _h2);            addChild(dl);            /*dl.loadData("http://localhost:3000/api/excluidas_vs_reportadas");*/            dl.loadData("http://esdata.info/api/excluidas_vs_reportadas");                        edo = new EtiquetaDinamica( 63,  70,    "Estado:" ); addChild( edo );            mun = new EtiquetaDinamica( 63,  85, "Municipio:" ); addChild( mun );            par = new EtiquetaDinamica( 63, 100, "Parroquia:" ); addChild( par );            ctr = new EtiquetaDinamica( 63, 115,    "Centro:" ); addChild( ctr );            msa = new EtiquetaDinamica( 63, 130,      "Mesa:" ); addChild( msa );            prc = new EtiquetaDinamica( 63, 145,"% Oficialista 2006:" ); addChild( prc );                        var fsb : FullScreenButton = new FullScreenButton(870,570);            addChild(fsb);        }                public function erraseLabels() : void {            edo.setLabelValue( "" );            mun.setLabelValue( "" );            par.setLabelValue( "" );            ctr.setLabelValue( "" );            msa.setLabelValue( "" );            prc.setLabelValue( "" );        }                public function setLabels(cod_centro:String, mesa:uint, percentage:Number) : void {            edo.setLabelValue(     Estado.Nombre( cod_centro ) );            mun.setLabelValue(  Municipio.Nombre( cod_centro ) );            par.setLabelValue(  Parroquia.Nombre( cod_centro ) );            ctr.setLabelValue(     Centro.Nombre( cod_centro ) );            msa.setLabelValue(       mesa.toString( ) );            prc.setLabelValue( percentage.toFixed(2)+" %" );        }        public function h1onTop() : void {            removeChild(_h1);            removeChild(_h2);            addChild(_h1);            addChild(_h2);            _h1.mouseChildren = true;            _h2.mouseChildren = false;            _h1.alpha = 1;            _h2.alpha = 0.2;            _h2.mouseEnabled = false;            _h1.mouseEnabled = true;            _h2.deselectActiveBinItem();            _ds.leyenda1.alpha = 1            _ds.leyenda2.alpha = 0.2        }                public function h2onTop() : void {            removeChild(_h1);            removeChild(_h2);            addChild(_h2);                        addChild(_h1);            _h2.mouseChildren = true;            _h1.mouseChildren = false;            _h2.alpha = 1;            _h1.alpha = 0.2;            _h2.mouseEnabled = true;            _h1.mouseEnabled = false;               _h1.deselectActiveBinItem();            _ds.leyenda2.alpha = 1            _ds.leyenda1.alpha = 0.2        }    }}